#include <iostream>
#include <vector>

int main() {

srand((time(null));

    vector<int> A(100);
    for (int i = 0; i < 100; i++) {
        A[i] = rand() % 100;
    }

    vector<int> B = A;
    int cantidadUnos = 0;

    for (int& elemento : B) {
        if (elemento % 2 == 0 && elemento > 20) {
            elemento *= 10;
        } else if (elemento % 2 != 0 && elemento < 60) {
            elemento -= 30;
        }

        if (elemento == 1) {
            cantidadUnos++;
        }
    }

    
    cout << "Vector A: ";
    for (int elemento : A) {
    cout << elemento << " ";
    }
    cout << endl;

    cout << "Vector B: ";
    for (int elemento : B) {
    cout << elemento << " ";
    }
    cout << endl;

    cout << "Cantidad de elementos '1' en B: " << cantidadUnos <<endl;

    return 0;
}

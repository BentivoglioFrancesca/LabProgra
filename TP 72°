#include <iostream>
#include <vector>

using namespace std;

vector<int> SumarVectores(const vector<int>& A, const vector<int>& B) {
    vector<int> C(A.size());

    for (int i = 0; i < A.size(); i++) {
        C[i] = A[i] + B[i];
    }

    return C;
}

int main() {
    srand((time(nullptr));

    vector<int> A(10);
    vector<int> B(10);

    for (int i = 0; i < 10; i++) {
        A[i] = rand() % 1500;
        B[i] = rand() % 1500;
    }

    vector<int> C = SumarVectores(A, B);

    cout << "Vector A: ";
    for (int num : A) {
        cout << num << " ";
    }
    cout << endl;

    cout << "Vector B: ";
    for (int num : B) {
        cout << num << " ";
    }
    cout << endl;

    cout << "Vector C (A + B): ";
    for (int num : C) {
        cout << num << " ";
    }
    cout << endl;

    return 0;
}
